#include<iostream>
#include<fstream>
#include<map>
#include<string>
using namespace std;

const int SIZE = 91;
const int BOOK_NUMBER = 1733444;
const int BOOK_CATEGROY = 0;

int initialScore[SIZE];
int trainScore[SIZE];
int validationScore[SIZE];

void InputScore () {
	freopen("D:\\DTLDownLoads\\data\\成绩排名预测\\训练\\成绩.txt", "r", stdin);
	//freopen ("D:\\DTLDownLoads\\data\\成绩排名预测\\训练\\result.csv", "w", stdout);
	
	string t,i,re;
	cin >> t >> i >> re;
	for (int i = 0; i < 3*SIZE; i++) {
		int term, id, so;
		cin >> term >> id >> so;
		if (term == 1) {
			initialScore[id] = so;
		}else if (term == 2) {
			trainScore[id] = so;
		}else
			validationScore[id] = so;
	}
}



void GetProBook () {

	freopen("D:\\DTLDownLoads\\data\\成绩排名预测\\训练\\图书类别.txt", "r", stdin);
	map<int, string> ma;
	map<string, int> category;
	string id, ca;
	cin >> id >> ca;
	int counter = 0;
	for (int i = 0; i < BOOK_NUMBER; i++) {
		int id;
		string s;
		cin >> id >> s;
		if (category[s] == 0) {
			category[s] = counter;
			counter++;
		}
	}
}
int main () {
	
	//freopen ("D:\\DTLDownLoads\\data\\成绩排名预测\\训练\\result.csv", "w", stdout);
	
}
